{
  "name": "tradi-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "del-cli dist .vite coverage",
    "build": "pnpm run clean && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "dataset": "node ./set-date-modified.cjs",
    "check": "pnpm run dataset && pnpm run clean && pnpm install && pnpm run format && pnpm run lint:fix && pnpm run typecheck && echo ✅ Alles sauber, Chef!",
    "preview": "vite preview",
    "analyze": "vite build --sourcemap && source-map-explorer dist/assets/*.js",
    "reset": "pnpm run clean && rm -rf node_modules && pnpm install",
    "prelive": "node -e \"const b = require('child_process').execSync('git rev-parse --abbrev-ref HEAD').toString().trim(); if (b !== 'main') { console.error(`❌ Du bist nicht auf 'main' - sondern auf '${b}'`); process.exit(1) } else { console.log('✅ Auf main - weiter geht\\'s!') }\"",
    "live": "pnpm run prelive && bash ./deploy.sh"
  },
  "dependencies": {
