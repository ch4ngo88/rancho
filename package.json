    "leaflet": "^1.9.4",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.56.4",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
